{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.29/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.yaml",
  "mouse_follows_focus": false,
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 8,
  "default_container_padding": 10,
  "border": true,
  "border_width": 0,
  "border_offset": -1,
  "floating_applications": [
    {
      "kind": "Class",
      "id": "Engine",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "Mullvad VPN.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "RGui.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "Neurontube.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "anki.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "Microsoft.CmdPal.UI.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Class",
      "id": "RAIL_WINDOW",
      "matching_strategy": "Equals"
    }
  ],
  "object_name_change_applications": [
    {
      "kind": "Class",
      "id": "Engine",
      "matching_strategy": "Equals"
    }
  ],
  "theme": {
    "palette": "Catppuccin",
    "name": "Mocha",
    "unfocused_border": "Base",
    "bar_accent": "Lavender"
  },
  "stackbar": {
    "height": 40,
    "mode": "OnStack",
    "tabs": {
      "width": 300
    }
  },
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP"
        },
        {
          "name": "II",
          "layout": "VerticalStack"
        },
        {
          "name": "III",
          "layout": "VerticalStack"
        },
        {
          "name": "IV",
          "layout": "VerticalStack"
        },
        {
          "name": "V",
          "layout": "BSP"
        },
        {
          "name": "VI",
          "layout": "BSP"
        },
        {
          "name": "VII",
          "layout": "BSP"
        }
      ]
    }
  ]
}
